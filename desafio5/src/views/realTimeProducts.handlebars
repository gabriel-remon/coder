<body>
    <table>
  <thead>
    <tr>
      <th>Título</th>
      <th>Descripción</th>
      <th>Precio</th>
      <th>Stock</th>
      <th>Code</th>
    </tr>
  </thead>
  <tbody>
    {{#each products}}
    <tr>
      <td>{{this.title}}</td>
      <td>{{this.description}}</td>
      <td>{{this.price}}</td>
      <td>{{this.stock}}</td>
      <td>{{this.code}}</td>
    </tr>
    {{/each}}
  </tbody>
  </table>
</body>
<script src="/socket.io/socket.io.js"></script>
<script src="/index.js"></script>